{
  "name": "unison",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_top_level_declaration"
      }
    },
    "_top_level_declaration": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_expression"
        },
        {
          "type": "SYMBOL",
          "name": "term_declaration"
        }
      ]
    },
    "type_signature": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "name"
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "SYMBOL",
          "name": "_type"
        }
      ]
    },
    "term_declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "type_signature"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "term_definition"
        }
      ]
    },
    "term_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "name"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "param"
          }
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "_expression"
        }
      ]
    },
    "param": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "name"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_type"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "_expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_literal"
        }
      ]
    },
    "_literal": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "boolean_literal"
        },
        {
          "type": "SYMBOL",
          "name": "byte_literal"
        },
        {
          "type": "SYMBOL",
          "name": "char_literal"
        },
        {
          "type": "SYMBOL",
          "name": "text_literal"
        },
        {
          "type": "SYMBOL",
          "name": "natural_literal"
        },
        {
          "type": "SYMBOL",
          "name": "float_literal"
        }
      ]
    },
    "boolean_literal": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "true"
        },
        {
          "type": "STRING",
          "value": "false"
        }
      ]
    },
    "byte_literal": {
      "type": "PATTERN",
      "value": "0xs[0-9a-f]+"
    },
    "char_literal": {
      "type": "PATTERN",
      "value": "\\?[\u0000-ô¿¿]"
    },
    "text_literal": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "\""
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "PATTERN",
                    "value": "[^\"\\\\\\n]"
                  },
                  {
                    "type": "PATTERN",
                    "value": "\\\\."
                  }
                ]
              }
            },
            {
              "type": "STRING",
              "value": "\""
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "\"\"\""
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "PATTERN",
                    "value": "[^\"\\\\]"
                  },
                  {
                    "type": "PATTERN",
                    "value": "\\\\."
                  }
                ]
              }
            },
            {
              "type": "STRING",
              "value": "\"\"\""
            }
          ]
        }
      ]
    },
    "natural_literal": {
      "type": "PATTERN",
      "value": "[0-9]+"
    },
    "integer_literal": {
      "type": "PATTERN",
      "value": "[+-][0-9]+"
    },
    "float_literal": {
      "type": "PATTERN",
      "value": "[+-]?[0-9]+\\.[0-9]+"
    },
    "name": {
      "type": "PATTERN",
      "value": "[a-zA-Z][a-zA-Z0-9_]*"
    },
    "_type": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "type_variable"
        },
        {
          "type": "SYMBOL",
          "name": "type_polymorphic"
        },
        {
          "type": "SYMBOL",
          "name": "type_builtin"
        },
        {
          "type": "SYMBOL",
          "name": "type_builtin_constructor"
        }
      ]
    },
    "type_variable": {
      "type": "PATTERN",
      "value": "[a-z]+"
    },
    "type_polymorphic": {
      "type": "PATTERN",
      "value": "[A-Z]+"
    },
    "type_builtin": {
      "type": "CHOICE",
      "members": [
